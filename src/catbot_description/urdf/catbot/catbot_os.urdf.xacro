      rpy="1.570796326794896558 5.0445758681405589749e-15 1.570796326794896336" />
      <parent link="still_chassis_assm" />
      <child link="wheel_assm_2" />
      <axis xyz="0 0 1" />
      <limit effort="1" velocity="20" />
      <joint_properties friction="0.0" />
