          <mesh filename="package://catbot_description/meshes/visual/wheel_assm_3_visual.stl" />
        </geometry>
        <material name="wheel_assm_3_material">
          <color rgba="0.34876043041436383119 0.28454440741211711741 0.12249285133838178075 1.0" />
